{
    "name": "kcloze/swoole-notify",
    "type": "library",
    "description": "基于swoole的消息异步中间件，构建基于异步消息驱动后台模型",
    "keywords": [
        "kcloze",
        "swoole-notify"
    ],
    "homepage": "https://github.com/kcloze/swoole-notify",
    "license": "MIT",
    "authors": [
        {
            "name": "kcloze",
            "email": "pei.greet@gmail.com",
            "homepage": "https://github.com/kcloze",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Kcloze\\Notify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kcloze\\Notify\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
